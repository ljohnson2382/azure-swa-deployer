{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "_comment": "Sample parameter file. Copy to infra/parameters-<client>-<env>.json and update values before deploying.",
  "parameters": {
    "staticWebAppName": {
      "value": "swa-<client>-<env>"
    },
    "location": {
      "value": "West US 2"
    },
    "repositoryUrl": {
      "value": "https://github.com/<org>/<repo>"
    },
    "repositoryBranch": {
      "value": "main"
    },
    "appLocation": {
      "value": "/"
    },
    "apiLocation": {
      "value": "api"
    },
    "outputLocation": {
      "value": "dist"
    },
    "sku": {
      "value": "Free"
    },
    "tags": {
      "value": {
        "Environment": "dev",
        "Client": "<client>",
        "CostCenter": "<cost-center>",
        "Owner": "<team>",
        "Project": "<project-name>"
      }
    },
    "allowConfigFileUpdates": {
      "value": true
    },
    "enterpriseGradeCdnStatus": {
      "value": "Disabled"
    },
    "enableGitHubIntegration": {
      "value": false
    },
    "enableBasicAuth": {
      "value": false
    }
  }
}